<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="QRCodeView">
        <!-- 扫描框距离toolbar底部的距离，默认值为90dp -->
        <attr name="qrcv_topOffset" format="dimension|reference" />
        <!-- 扫描框边角线的宽度，默认值为3dp -->
        <attr name="qrcv_cornerSize" format="dimension|reference" />
        <!-- 扫描框边角线的长度，默认值为20dp -->
        <attr name="qrcv_cornerLength" format="dimension|reference" />
        <!-- 扫描框边角线的颜色，默认值为@android:color/white -->
        <attr name="qrcv_cornerColor" format="color|reference" />
        <!-- 扫描框的宽度，默认值为200dp -->
        <attr name="qrcv_rectWidth" format="dimension|reference" />
        <!-- 除去扫描框，其余部分阴影颜色，默认值为#33FFFFFF -->
        <attr name="qrcv_maskColor" format="color|reference" />
        <!-- 扫描线的宽度，默认值为1dp -->
        <attr name="qrcv_scanLineSize" format="dimension|reference" />
        <!-- 扫描线的颜色，默认值为@android:color/white -->
        <attr name="qrcv_scanLineColor" format="color|reference" />
        <!-- 扫描线距离上下或者左右边框的间距，默认值为0dp -->
        <attr name="qrcv_scanLineMargin" format="dimension|reference" />
        <!-- 是否显示默认的图片扫描线，默认值为false -->
        <attr name="qrcv_isShowDefaultScanLineDrawable" format="boolean" />
        <!-- 扫描线的图片资源，默认值为null -->
        <attr name="qrcv_customScanLineDrawable" format="reference" />
        <!-- 扫描边框的宽度，默认值为1dp -->
        <attr name="qrcv_borderSize" format="dimension|reference" />
        <!-- 扫描边框的颜色，默认值为@android:color/white -->
        <attr name="qrcv_borderColor" format="color|reference" />
        <!-- 扫描线从顶部移动到底部的动画时间，默认值为1000 -->
        <attr name="qrcv_animTime" format="integer" />
        <!-- 扫描框是否垂直居中，该属性为true时会忽略qrcv_topOffset属性，默认值为false -->
        <attr name="qrcv_isCenterVertical" format="boolean" />
        <!-- Toolbar的高度，当有设置qrcv_isCenterVertical属性时，通过该属性来修正有Toolbar时导致扫描框垂直居中的偏差，默认值为0dp -->
        <attr name="qrcv_toolbarHeight" format="dimension|reference" />
        <attr name="qrcv_isScanLineReverse" format="boolean" />
        <!-- 扫描线的网格图片资源，默认值为null -->
        <attr name="qrcv_customGridScanLineDrawable" format="reference" />
    </declare-styleable>

    <!-- 自定义扫码 -->
    <declare-styleable name="ScannerItemView">
        <!-- 左侧文字 -->
        <attr name="sciv_left_text" format="string"></attr>
        <!-- 右侧图片 -->
        <attr name="sciv_right_icon" format="integer"></attr>
    </declare-styleable>

    <declare-styleable name="LeftRightItemView">
        <attr name="left_text" format="string" />
        <attr name="right_text" format="string" />
        <attr name="lrv_padding" format="dimension|reference" />
        <attr name="lrv_left_width" format="dimension|reference" />
        <attr name="lrv_left_color" format="color" />
        <attr name="lrv_right_color" format="color" />
        <attr name="lrv_right_icon" format="reference"></attr>
    </declare-styleable>

    <declare-styleable name="LoopView">
        <attr name="lineColor" format="color" />
        <attr name="topBottomTextColor" format="color" />
        <attr name="centerTextColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="canLoop" format="boolean" />
        <attr name="initPosition" format="integer" />
        <attr name="drawItemCount" format="integer" />
    </declare-styleable>

    <declare-styleable name="ExpandingCollection">
        <attr name="cardWidth" format="dimension" />
        <attr name="cardHeight" format="dimension" />
        <attr name="cardHeaderHeightExpanded" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TextFieldBoxes">

        <!--Texts-->
        <attr name="labelText" format="string" />
        <attr name="helperText" format="string" />

        <!--Colors-->
        <attr name="helperTextColor" format="color" />
        <attr name="counterTextColor" format="color" />
        <attr name="errorColor" format="color" />
        <attr name="primaryColor" format="color" />
        <attr name="secondaryColor" format="color" />
        <attr name="panelBackgroundColor" format="color" />

        <!--Characters counter-->
        <attr name="maxCharacters" format="integer" />
        <attr name="minCharacters" format="integer" />

        <!--Others-->
        <attr name="enabled" format="boolean" />
        <attr name="iconSignifier" format="reference" />
        <attr name="endIcon" format="reference" />
        <attr name="isResponsiveIconColor" format="boolean" />
        <attr name="hasClearButton" format="boolean" />
        <attr name="hasJiaJian" format="boolean"></attr>
        <attr name="hasFocus" format="boolean" />
        <attr name="alwaysShowHint" format="boolean" />
        <attr name="useDenseSpacing" format="boolean" />
        <attr name="selected" format="boolean" />

        <!-- Set whether to automaticavalidateErrordate, or only on demand
               @see {@link TextFieldBoxes#setManualValidateError(boolean) } -->
        <attr name="manualValidateError" format="boolean" />

    </declare-styleable>

    <declare-styleable name="ExtendedEditText">

        <!--Texts-->
        <attr name="prefix" format="string" />
        <attr name="suffix" format="string" />

        <!--Colors-->
        <attr name="prefixTextColor" format="color" />
        <attr name="suffixTextColor" format="color" />

    </declare-styleable>

    <!-- ActionMenu Property -->
    <declare-styleable name="ActionMenu">
        <attr name="circleRadius" format="dimension" />
        <attr name="dimens" format="dimension" />
        <attr name="animationDuration" format="integer" />
        <attr name="buttonNormalColor" format="color" />
        <attr name="buttonPressColor" format="color" />
        <attr name="actionMenuIcon" format="reference" />
        <attr name="actionMenuOnIcon" format="reference" />
        <attr name="expandDirect" />
    </declare-styleable>

    <!-- direct enum -->
    <attr name="expandDirect">
        <enum name="expandDirectTop" value="0" />
        <enum name="expandDirectDown" value="1" />
        <enum name="expandDirectLeft" value="2" />
        <enum name="expandDirectRight" value="3" />
    </attr>

    <!-- 卫星菜单 -->
    <declare-styleable name="SatelliteMenu">
        <attr name="radius" format="dimension"></attr>
        <attr name="menu_image_width" format="dimension"></attr>
        <attr name="menu_item_image_width" format="dimension"></attr>
        <attr name="menu_item_text_size" format="dimension"></attr>
        <attr name="menu_item_text_color" format="color"></attr>

        <attr name="menu_postion" format="integer">
            <enum name="left_top" value="0"></enum>
            <enum name="right_top" value="1"></enum>
            <enum name="left_bottom" value="2"></enum>
            <enum name="right_bottom" value="3"></enum>
        </attr>
    </declare-styleable>

    <declare-styleable name="FabWithLabelView">
        <attr name="android:src" />
        <attr name="srcCompat" />
        <attr name="fabLabel" format="string" />
        <attr name="fabBackgroundColor" format="color" />
        <attr name="fabLabelColor" format="color" />
        <attr name="fabLabelBackgroundColor" format="color" />
        <attr name="fabLabelClickable" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SpeedDialView">
        <attr name="sdExpansionMode" format="enum">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="left" value="2" />
            <enum name="right" value="3" />
        </attr>
        <attr name="sdMainFabAnimationRotateAngle" format="float" />
        <attr name="sdMainFabClosedSrc" format="reference" />
        <attr name="sdMainFabClosedBackgroundColor" format="color" />
        <attr name="sdMainFabOpenedSrc" format="reference" />
        <attr name="sdMainFabOpenedBackgroundColor" format="color" />
        <attr name="sdOverlayLayout" format="reference" />
        <attr name="sdUseReverseAnimationOnClose" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SpeedDialOverlayLayout">
        <attr name="clickable_overlay" format="boolean" />
        <attr name="android:background" />
    </declare-styleable>

    <attr name="icvp_min_page_scale_offset" format="dimension" />
    <attr name="icvp_center_page_scale_offset" format="dimension" />
    <attr name="icvp_min_page_scale" format="float" />
    <attr name="icvp_max_page_scale" format="float" />
    <attr name="icvp_medium_scaled" format="boolean" />
    <attr name="icvp_scroll_duration" format="integer" />
    <attr name="icvp_page_duration" format="integer" />
    <attr name="icvp_interpolator" format="reference" />

    <declare-styleable name="HorizontalInfiniteCycleViewPager">
        <attr name="icvp_min_page_scale_offset" />
        <attr name="icvp_center_page_scale_offset" />
        <attr name="icvp_min_page_scale" />
        <attr name="icvp_max_page_scale" />
        <attr name="icvp_medium_scaled" />
        <attr name="icvp_scroll_duration" />
        <attr name="icvp_page_duration" />
        <attr name="icvp_interpolator" />
    </declare-styleable>

    <declare-styleable name="VerticalInfiniteCycleViewPager">
        <attr name="icvp_min_page_scale_offset" />
        <attr name="icvp_center_page_scale_offset" />
        <attr name="icvp_min_page_scale" />
        <attr name="icvp_max_page_scale" />
        <attr name="icvp_medium_scaled" />
        <attr name="icvp_scroll_duration" />
        <attr name="icvp_page_duration" />
        <attr name="icvp_interpolator" />
    </declare-styleable>

    <declare-styleable name="CustomCircleProgressBar">
        <attr name="outside_color" format="color" />
        <attr name="outside_radius" format="dimension" />
        <attr name="inside_color" format="color" />
        <attr name="progress_text_color" format="color" />
        <attr name="progress_text_size" format="dimension" />
        <attr name="progress_width" format="dimension" />
        <attr name="max_progress" format="integer" />
        <attr name="progress" format="float" />
        <attr name="direction">
            <enum name="left" value="0" />
            <enum name="top" value="1" />
            <enum name="right" value="2" />
            <enum name="bottom" value="3" />
        </attr>
    </declare-styleable>
</resources>